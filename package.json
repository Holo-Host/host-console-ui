{
  "name": "host-console",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "serve": "VUE_APP_UI_VERSION=$npm_package_version vue-cli-service serve",
    "build": "VUE_APP_UI_VERSION=$npm_package_version VUE_APP_USE_REAL_PUB_KEY=true vue-cli-service build",
